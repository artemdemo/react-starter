    "style-loader": "^3.3.1",
    "ts-loader": "^9.0.0",
    "typescript": "^4.8.0",
    "webpack": "^5.76.0",
    "webpack-bundle-analyzer": "^4.0.0",
    "webpack-cli": "^4.0.0",
    "webpack-dev-server": "^4.0.0"
  }
