{
  "name": "app",
  "version": "1.0.0",
  "description": "Basic packages and scripts to start app developing in react, redux and webpack",
  "main": "webpack.config.js",
  "scripts": {
    "start": "webpack serve",
    "build": "NODE_ENV=production webpack --progress",
    "test": "run -T jest",
    "test:coverage": "BABEL_DISABLE_CACHE=1 jest --config ./jest/jest.config.js --coverage",
    "lint": "prettier --write .",
    "stats": "BABEL_ENV=production webpack --profile --json > bundle-stats.json && webpack-bundle-analyzer bundle-stats.json -p 8888 -r bundle-report.html"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.2.0",
    "@fortawesome/free-solid-svg-icons": "^6.2.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "axios": "^0.27.2",
    "bootstrap": "^5.2.1",
    "classnames": "^2.3.2",
    "cssnano": "^5.0.13",
    "lodash": "^4.17.21",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-router-dom": "^6.0.2",
    "redux": "^4.0.5",
    "redux-actions": "^2.6.5",
    "redux-saga": "^1.1.3"
  },
  "devDependencies": {
    "@testing-library/react": "^12.1.2",
    "@types/lodash": "^4.14.186",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.11",
    "@types/react-router-dom": "^5.1.7",
    "@types/redux-actions": "^2.6.2",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "latest",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^5.2.0",
    "postcss-loader": "latest",
    "prettier": "^2.7.1",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^17.0.1",
    "redux-mock-store": "^1.5.4",
    "style-loader": "^3.3.1",
    "ts-loader": "latest",
    "typescript": "^4.2.3",
    "webpack": "latest",
    "webpack-bundle-analyzer": "latest",
    "webpack-cli": "latest",
    "webpack-dev-server": "latest"
  },
  "packageManager": "yarn@3.2.3"
}
