{
  "name": "app",
  "version": "1.0.0",
  "description": "Basic packages and scripts to start app developing in react, redux and webpack",
  "main": "webpack.config.js",
  "scripts": {
    "start": "webpack serve",
    "build": "NODE_ENV=production webpack --progress",
    "test": "run -T vitest run",
    "test:watch": "run -T vitest watch",
    "lint": "prettier --write .",
    "stats": "BABEL_ENV=production webpack --profile --json > bundle-stats.json && webpack-bundle-analyzer bundle-stats.json -p 8888 -r bundle-report.html"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.2.0",
    "@fortawesome/free-solid-svg-icons": "^6.2.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "axios": "^0.27.2",
    "bootstrap": "^5.2.1",
    "classnames": "^2.3.2",
    "cssnano": "^5.0.13",
    "lodash": "^4.17.21",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-redux": "^8.0.0",
    "react-router-dom": "^6.4.1",
    "redux": "^4.2.0",
    "redux-actions": "^2.6.5",
    "redux-saga": "^1.2.1"
  },
  "devDependencies": {
    "@testing-library/react": "^13.4.0",
    "@types/lodash": "^4.14.186",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/react-router-dom": "^5.1.7",
    "@types/redux-actions": "^2.6.2",
    "clean-webpack-plugin": "^4.0.0",
