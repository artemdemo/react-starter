{
  "name": "react-webpack-starter",
  "version": "8.2.0",
  "description": "Basic packages and scripts to start app developing in react, redux and webpack",
  "main": "webpack.config.js",
  "scripts": {
    "start": "./start.sh",
    "server:dev": "webpack-dev-server",
    "server:dev:minified": "MINIFIED=true webpack-dev-server --client-log-level none",
    "build": "BABEL_ENV=production webpack --progress --colors",
    "build:watch": "BABEL_ENV=production webpack --progress --colors --watch",
    "test": "BABEL_DISABLE_CACHE=1 jest --config ./jest/jest.config.js",
    "test:coverage": "BABEL_DISABLE_CACHE=1 jest --config ./jest/jest.config.js --coverage",
    "eslint": "eslint --quiet ./source/**/*.js ./source/**/**/*.js ./source/**/*.jsx ./source/**/**/*.jsx",
    "stats": "BABEL_ENV=production webpack --profile --json > bundle-stats.json && webpack-bundle-analyzer bundle-stats.json -p 8888 -r bundle-report.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/artemdemo/react-webpack-starter.git"
  },
  "author": "Artem Demo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/artemdemo/react-webpack-starter/issues"
  },
  "homepage": "https://github.com/artemdemo/react-webpack-starter#readme",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "@types/jest": "^26.0.14",
    "@types/prop-types": "^15.7.3",
    "@types/react": "^16.9.49",
    "@types/styled-components": "^5.1.3",
    "axios": "^0.19.2",
    "bootstrap": "^4.5.0",
    "classnames": "^2.2.5",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^4.3.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^6.1.0",
    "history": "^4.10.1",
    "html-webpack-plugin": "^4.4.1",
    "less": "^3.11.1",
    "less-loader": "^6.1.0",
    "less-plugin-clean-css": "^1.5.1",
    "lodash": "^4.17.20",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "redux-actions": "^2.6.5",
    "redux-saga": "^1.1.3",
    "style-loader": "^1.1.3",
    "styled-components": "^5.2.0",
    "ts-jest": "^26.3.0",
    "ts-loader": "^8.0.3",
    "typescript": "^4.0.2",
    "typescript-plugin-styled-components": "^1.4.4",
    "url-loader": "^4.1.0",
    "webpack": "^4.44.2",
    "webpack-chunk-hash": "^0.6.0",
    "webpack-cli": "^3.3.12"
  },
  "devDependencies": {
    "jest-cli": "^26.4.2",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^16.13.1",
    "redux-mock-store": "^1.5.4",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-dev-server": "^3.10.3"
  }
}
