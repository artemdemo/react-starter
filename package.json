{
  "name": "react-webpack-starter",
  "version": "8.1.0",
  "description": "Basic packages and scripts to start app developing in react, redux and webpack",
  "main": "webpack.config.js",
  "scripts": {
    "start": "./start.sh",
    "server:dev": "webpack-dev-server",
    "server:dev:minified": "MINIFIED=true webpack-dev-server --client-log-level none",
    "build": "BABEL_ENV=production webpack --progress --colors",
    "test": "BABEL_DISABLE_CACHE=1 jest --config ./jest/jest.config.js",
    "test:coverage": "BABEL_DISABLE_CACHE=1 jest --config ./jest/jest.config.js --coverage",
    "eslint": "eslint --quiet ./source/**/*.js ./source/**/**/*.js ./source/**/*.jsx ./source/**/**/*.jsx",
    "stats": "BABEL_ENV=production webpack --profile --json > bundle-stats.json && webpack-bundle-analyzer bundle-stats.json -p 8888 -r bundle-report.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/artemdemo/react-webpack-starter.git"
  },
  "author": "Artem Demo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/artemdemo/react-webpack-starter/issues"
  },
  "homepage": "https://github.com/artemdemo/react-webpack-starter#readme",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/react-fontawesome": "^0.1.9",
    "axios": "^0.19.2",
    "bootstrap": "^4.5.0",
    "classnames": "^2.2.5",
    "history": "^4.10.1",
    "lodash": "^4.17.15",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "redux-actions": "^2.6.5",
    "redux-saga": "^1.1.3",
    "styled-components": "^5.1.0"
  },
  "devDependencies": {
    "@types/jest": "^25.2.2",
    "@types/prop-types": "^15.7.3",
    "@types/react": "^16.9.35",
    "@types/styled-components": "^5.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.5.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.2.0",
    "jest-cli": "^25.3.0",
    "less": "^3.11.1",
    "less-loader": "^6.1.0",
    "less-plugin-clean-css": "^1.5.1",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^16.13.1",
    "redux-mock-store": "^1.5.4",
    "style-loader": "^1.1.3",
    "ts-jest": "^25.5.1",
    "ts-loader": "^7.0.4",
    "typescript": "^3.9.2",
    "typescript-plugin-styled-components": "^1.4.4",
    "url-loader": "^4.1.0",
    "webpack": "^4.42.1",
    "webpack-bundle-analyzer": "^3.7.0",
    "webpack-chunk-hash": "^0.6.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  }
}
