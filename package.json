{
  "name": "react-webpack-starter",
  "version": "7.4.0",
  "description": "Basic packages and scripts to start app developing in react, redux and webpack",
  "main": "webpack.config.js",
  "scripts": {
    "start": "./start.sh",
    "server:dev": "webpack-dev-server",
    "server:dev:minified": "MINIFIED=true webpack-dev-server --client-log-level none",
    "server:prod": "NODE_ENV=production npm run build && http-server ./build",
    "build": "BABEL_ENV=production webpack --progress --colors",
    "test": "BABEL_DISABLE_CACHE=1 jest --config ./jest/jest-config.json",
    "test:coverage": "BABEL_DISABLE_CACHE=1 jest --config ./jest/jest-config.json --coverage",
    "eslint": "eslint --quiet ./source/**/*.js ./source/**/**/*.js ./source/**/*.jsx ./source/**/**/*.jsx",
    "stats": "BABEL_ENV=production webpack --profile --json > bundle-stats.json && webpack-bundle-analyzer bundle-stats.json -p 8888 -r bundle-report.html",
    "postinstall": "./postinstall.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/artemdemo/react-webpack-starter.git"
  },
  "author": "Artem Demo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/artemdemo/react-webpack-starter/issues"
  },
  "homepage": "https://github.com/artemdemo/react-webpack-starter#readme",
  "dependencies": {
    "axios": "^0.19.0",
    "babel-plugin-dynamic-import-node": "^2.3.0",
    "bluebird": "^3.5.5",
    "bootstrap": "^4.3.1",
    "classnames": "^2.2.5",
    "font-awesome": "^4.7.0",
    "history": "^4.9.0",
    "lodash": "^4.17.15",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.4",
    "redux-actions": "^2.6.5",
    "redux-saga": "^1.0.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-modules-commonjs": "^7.5.0",
    "@babel/plugin-transform-object-assign": "^7.2.0",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.5.5",
    "@types/jest": "^24.0.17",
    "@types/prop-types": "^15.7.1",
    "@types/react": "^16.9.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint-config-artem-react": "git+https://github.com/artemdemo/eslint-config-artem-react.git#3.1.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.11.1",
    "jest-cli": "^24.7.1",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "less-plugin-clean-css": "^1.5.1",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^16.9.0",
    "redux-mock-store": "^1.5.1",
    "style-loader": "^1.0.0",
    "url-loader": "^2.1.0",
    "webpack": "^4.39.2",
    "webpack-bundle-analyzer": "^3.4.1",
    "webpack-chunk-hash": "^0.6.0",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.8.0"
  }
}
