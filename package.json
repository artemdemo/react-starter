{
  "name": "react-webpack-starter",
  "version": "9.1.0",
  "description": "Basic packages and scripts to start app developing in react, redux and webpack",
  "main": "webpack.config.js",
  "scripts": {
    "start": "./start.sh",
    "server:dev": "webpack serve --mode development",
    "build": "NODE_ENV=production webpack --progress",
    "test": "BABEL_DISABLE_CACHE=1 jest --config ./jest/jest.config.js",
    "test:coverage": "BABEL_DISABLE_CACHE=1 jest --config ./jest/jest.config.js --coverage",
    "lint": "prettier --write .",
    "stats": "BABEL_ENV=production webpack --profile --json > bundle-stats.json && webpack-bundle-analyzer bundle-stats.json -p 8888 -r bundle-report.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/artemdemo/react-webpack-starter.git"
  },
  "author": "Artem Demo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/artemdemo/react-webpack-starter/issues"
  },
  "homepage": "https://github.com/artemdemo/react-webpack-starter#readme",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.34",
    "@fortawesome/free-solid-svg-icons": "^5.15.2",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "axios": "^0.24.0",
    "bootstrap": "^5.1.3",
    "classnames": "^2.2.5",
    "cssnano": "^5.0.11",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-router-dom": "^6.0.2",
    "redux": "^4.0.5",
    "redux-actions": "^2.6.5",
    "redux-saga": "^1.1.3"
  },
  "devDependencies": {
    "@testing-library/react": "^12.1.2",
    "@types/jest": "^27.0.3",
    "@types/prop-types": "^15.7.3",
    "@types/react": "^17.0.2",
    "@types/react-router-dom": "^5.1.7",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.5.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^5.2.0",
    "jest-cli": "^27.3.1",
    "postcss-loader": "^6.2.1",
    "prettier": "2.5.0",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^17.0.1",
    "redux-mock-store": "^1.5.4",
    "style-loader": "^3.3.1",
    "ts-jest": "^27.0.7",
    "ts-loader": "^9.2.6",
    "typescript": "^4.2.3",
    "webpack": "^5.24.3",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^4.6.0"
  }
}
