{
  "name": "react-webpack-starter",
  "version": "3.2.1",
  "description": "Basic packages and scripts to start app developing in react, redux and webpack",
  "main": "webpack.config.js",
  "scripts": {
    "start": "npm run server:dev",
    "server:dev": "webpack-dev-server",
    "build": "webpack --progress --colors",
    "npm-check": "npm outdated",
    "test": "BABEL_DISABLE_CACHE=1 jest --config ./jest-config.json",
    "eslint": "./node_modules/eslint/bin/eslint.js ./source/**/*.js ./source/**/**/*.js ./source/**/*.jsx ./source/**/**/*.jsx",
    "watch": "webpack --progress --colors --watch",
    "stats": "webpack --profile --json > bundle-stats.json && webpack-bundle-analyzer bundle-stats.json -p 8888 -r bundle-report.html",
    "postinstall": "./postinstall.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/artemdemo/react-webpack-starter.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/artemdemo/react-webpack-starter/issues"
  },
  "homepage": "https://github.com/artemdemo/react-webpack-starter#readme",
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.22.0",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.22.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.22.0",
    "babel-runtime": "^6.25.0",
    "bootstrap": "^3.3.7",
    "classnames": "^2.2.5",
    "css-loader": "^0.28.4",
    "file-loader": "^0.11.2",
    "history": "^4.3.0",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.22.0",
    "less": "^2.7.1",
    "less-loader": "^4.0.4",
    "prop-types": "^15.5.10",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.5",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.6.0",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.7",
    "webpack": "^3.4.1"
  },
  "devDependencies": {
    "@types/jest": "^20.0.2",
    "@types/prop-types": "^15.5.1",
    "clean-webpack-plugin": "^0.1.15",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0",
    "eslint-config-artem-react": "git+https://github.com/artemdemo/eslint-config-artem-react.git#2.2.0",
    "jest-cli": "^20.0.4",
    "react-addons-test-utils": "^15.4.1",
    "react-test-renderer": "^15.5.4",
    "redux-mock-store": "^1.2.1",
    "webpack-bundle-analyzer": "^2.8.3",
    "webpack-chunk-hash": "^0.4.0",
    "webpack-dev-server": "^2.4.5"
  }
}
